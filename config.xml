<allow-intent href="sms:*" />
<allow-intent href="mailto:*" />
<allow-intent href="geo:*" />
<platform name="android">
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="32" />
    <icon density="mdpi" src="assets/mdpi/ic_akshara_logo.png" />
    <icon density="hdpi" src="assets/hdpi/ic_akshara_logo.png" />
    <icon density="xhdpi" src="assets/xhdpi/ic_akshara_logo.png" />
    <icon density="xxhdpi" src="assets/xxhdpi/ic_akshara_logo.png" />
    <allow-intent href="market:*" />
    <preference name="Fullscreen" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="AndroidInsecureFileModeEnabled" value="true" />
    <resource-file src="google-services.json" target="app/google-services.json" />
    <ignore entries="www/assets_server" />
